upstream REPLACEME {
  {{range service "REPLACEME"}}server {{ .Address }}:{{ .Port }};
  {{end}}
}
server {
  listen 80;
  listen 443 ssl spdy;
  include /etc/nginx/ssl.conf;
  server_name {{key "octohost/REPLACEME/DOMAINS"}};
  location / {
    include /etc/nginx/location.conf;
    proxy_pass http://REPLACEME;
  }
}
